    <div id="academic-container">
        <sl-details summary="Faith">
            <div id="divider">
                <sl-icon src={daisy.src}></sl-icon> <br>
            </div>
            <sl-icon slot="expand-icon" src={leaf.src}></sl-icon>
            <sl-icon slot="collapse-icon" src={leaf.src}></sl-icon>
